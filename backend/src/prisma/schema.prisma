generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["fullTextSearchPostgres"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Task {
  id           String   @id @default(uuid())
  serialNumber Int      @unique @default(autoincrement())
  title        String
  completed    Boolean
  createdAt    DateTime @default(now())
}
