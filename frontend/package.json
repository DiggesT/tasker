{
  "name": "@tasker/frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "types": "tsc --noEmit --project ./tsconfig.app.json && tsc --noEmit --project ./tsconfig.node.json",
    "stylecheck": "sass --update -q ./src:./node_modules/.cache/.stylecheckcache",
    "prettify": "prettier --log-level warn --cache --write '{src/**/*,*}.{ts,tsx,js,json,yml,scss}' '!**/{node_modules,dist}/**/*'"
  },
  "dependencies": {
    "@tasker/backend": "workspace:*",
    "@tanstack/react-query": "4.x",
    "@trpc/react-query": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "classnames": "^2.5.1",
    "formik": "^2.4.6",
    "formik-validator-zod": "^2.0.1",
    "include-media": "^2.0.0",
    "react": "^19.0.0",
    "react-infinite-scroller": "^1.2.6",
    "reset-css": "^5.0.2",
    "react-dom": "^19.0.0",
    "react-image-gallery": "^1.4.0",
    "react-router-dom": "^7.5.2",
    "sass": "^1.83.4",
    "superjson": "1.x"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/react-image-gallery": "^1.2.4",
    "@types/react-infinite-scroller": "^1.2.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1"
  }
}
